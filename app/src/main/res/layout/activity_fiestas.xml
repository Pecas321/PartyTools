<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fondo_fiestas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:fitsSystemWindows="true">

    <ImageView
        android:id="@+id/usuario_fiestas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usuario3" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="338dp"
        android:layout_height="134dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Bienvenido a la fiesta Cabito"
        android:textColor="#FF00F5"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usuario_fiestas" />

    <Button
        android:id="@+id/button_fiestasCerca"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#32CD32"
        android:text="Fiestas cerca"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/button_ComReuniones"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#A93BFF"
        android:text="Comunidad de reuniones"
        android:textColor="#050505"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_fiestascerca" />

    <TextView
        android:id="@+id/text_fiestascerca"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:gravity="center"
        android:text="Encuentra las fiestas mas cercanas a ti"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_fiestasCerca" />

    <TextView
        android:id="@+id/text_comunidadreuniones"
        android:layout_width="307dp"
        android:layout_height="69dp"
        android:gravity="center"
        android:text="Sin ideas? \n Encuentra inspiraciÃ³n aqui para tus reuniones"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_ComReuniones" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="410dp"
        android:layout_height="103dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/icono_fiestas_fiestas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView17"
        app:srcCompat="@drawable/pngtreegift_party_decoration_white_balloons_5790391" />

    <ImageView
        android:id="@+id/icono_alcohol_fiestas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintTop_toTopOf="@+id/textView17"
        app:srcCompat="@drawable/icono_cerveza2" />

    <ImageView
        android:id="@+id/icono_juegos_fiestas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/icono_alcohol_fiestas"
        app:layout_constraintTop_toTopOf="@+id/textView17"
        app:srcCompat="@drawable/icono_dado3" />

    <ImageView
        android:id="@+id/icono_cocteles_fiestas"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/icono_juegos_fiestas"
        app:layout_constraintTop_toTopOf="@+id/textView17"
        app:srcCompat="@drawable/icono_coctel2" />

    <TextView
        android:id="@+id/textofiestassfiestas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fiesta"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintStart_toStartOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintTop_toBottomOf="@+id/icono_fiestas_fiestas" />

    <TextView
        android:id="@+id/textofiestaalcohol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Alcohol"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/icono_alcohol_fiestas"
        app:layout_constraintHorizontal_bias="0.37"
        app:layout_constraintStart_toStartOf="@+id/icono_alcohol_fiestas"
        app:layout_constraintTop_toBottomOf="@+id/icono_alcohol_fiestas" />

    <TextView
        android:id="@+id/textofiestajuegos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Juegos"
        android:textColor="#FBFBFB"
        app:layout_constraintEnd_toEndOf="@+id/icono_juegos_fiestas"
        app:layout_constraintHorizontal_bias="0.649"
        app:layout_constraintStart_toStartOf="@+id/icono_juegos_fiestas"
        app:layout_constraintTop_toBottomOf="@+id/icono_juegos_fiestas" />

    <TextView
        android:id="@+id/textofiestacocteles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cocteles"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/icono_cocteles_fiestas"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/icono_cocteles_fiestas"
        app:layout_constraintTop_toBottomOf="@+id/icono_cocteles_fiestas" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:gravity="bottom|center"
        android:text="."
        android:textColor="#FFFFFF"
        android:textSize="96sp"
        app:layout_constraintBottom_toTopOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintEnd_toEndOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="@+id/icono_fiestas_fiestas"
        app:layout_constraintTop_toTopOf="@+id/textView17" />

</androidx.constraintlayout.widget.ConstraintLayout>