<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".Juegosdemesa">

    <TextView
        android:id="@+id/Juegodemesa"
        android:layout_width="212dp"
        android:layout_height="62dp"
        android:layout_marginTop="80dp"
        android:text="Juegos de mesa"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/uno"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="180dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/parques"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="180dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/hedbanz"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="351dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/domino"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="351dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/jenga"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="521dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="128dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/twuister"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="521dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="59dp"
        android:layout_height="28dp"
        android:gravity="center"
        android:text="Uno"
        android:textColor="#FBFAFA"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="291dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parques"
        android:textColor="#FFF9F9"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="292dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hedbanz"
        android:textColor="#FFFEFE"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Domino"
        android:textColor="#FDFCFC"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jenga"
        android:textColor="#FFFDFD"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="632dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Twister"
        android:textColor="#FDFDFD"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="632dp" />
</androidx.constraintlayout.widget.ConstraintLayout>